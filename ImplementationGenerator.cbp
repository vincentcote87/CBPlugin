<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="ImplementationGenerator" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="ImplementationGenerator" prefix_auto="0" extension_auto="1" />
				<Option object_output=".objs/default" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
					<Add option="`pkg-config --cflags codeblocks`" />
					<Add option="`wx-config --cflags  --version=2.8`" />
					<Add directory="$(cb.BASE)/include" />
					<Add directory="include" />
					<Add directory="src" />
				</Compiler>
				<Linker>
					<Add option="`pkg-config --libs codeblocks`" />
					<Add option="`wx-config --libs  --version=2.8`" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 ImplementationGenerator.zip manifest.xml" />
					<Add after="zip -j9 ImplementationGenerator.cbplugin ImplementationGenerator.so ImplementationGenerator.zip" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Target title="to_codeblocks_wx28">
				<Option output="$(#cb)/devel/share/codeblocks/plugins/Example" prefix_auto="1" extension_auto="1" />
				<Option working_dir="$(#cb)/devel" />
				<Option object_output=".objs/to_codeblocks_wx28" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance --no-splash-screen --verbose -p debug --no-ipc " />
				<Option host_application="$(#cb)/devel/codeblocks" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="`wx-config --cflags  --version=2.8`" />
					<Add option="-g" />
					<Add directory="$(#cb)/include" />
					<Add directory="$(#cb)/sdk/wxscintilla/include" />
					<Add directory="$(#cb)/include/tinyxml" />
				</Compiler>
				<Linker>
					<Add option="`wx-config --libs  --version=2.8`" />
					<Add library="codeblocks" />
					<Add directory="$(#cb)/devel" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 $(#cb)/devel/share/codeblocks/Example.zip manifest.xml" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Target title="to_codeblocks_wx30">
				<Option output="$(#cb)/devel30/share/codeblocks/plugins/Example" prefix_auto="1" extension_auto="1" />
				<Option working_dir="$(#cb)/devel30" />
				<Option object_output=".objs/to_codeblocks_wx30" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance --no-splash-screen --verbose -p debug --no-ipc " />
				<Option host_application="$(#cb)/devel30/codeblocks" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="`wx-config --cflags  --version=3.0`" />
					<Add option="-g" />
					<Add directory="$(#cb)/include" />
					<Add directory="$(#cb)/sdk/wxscintilla/include" />
					<Add directory="$(#cb)/include/tinyxml" />
				</Compiler>
				<Linker>
					<Add option="`wx-config --libs  --version=3.0`" />
					<Add library="codeblocks" />
					<Add directory="$(#cb)/devel30" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 $(#cb)/devel30/share/codeblocks/Example.zip manifest.xml" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Target title="RunTest">
				<Option output="testPlugin" prefix_auto="0" extension_auto="1" />
				<Option object_output=".objs/default" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
					<Add option="`pkg-config --cflags codeblocks`" />
					<Add option="`wx-config --cflags  --version=2.8`" />
					<Add directory="test" />
					<Add directory="$(cb.BASE)/include" />
					<Add directory="include" />
				</Compiler>
				<Linker>
					<Add option="-lgtest" />
					<Add option="-lpthread" />
					<Add option="/usr/src/gmock/gmock-all.cc" />
					<Add option="`pkg-config --libs codeblocks`" />
					<Add option="`wx-config --libs  --version=2.8`" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 ImplementationGenerator.zip manifest.xml" />
					<Add after="zip -j9 ImplementationGenerator.cbplugin ImplementationGenerator.so ImplementationGenerator.zip" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;to_codeblocks_wx28;to_codeblocks_wx30;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-std=c++11" />
			<Add option="-fPIC" />
		</Compiler>
		<Linker>
			<Add option="-Wl,--no-undefined" />
		</Linker>
		<Unit filename="include/CBImplementationBuilder.h" />
		<Unit filename="include/FileImplementationBuilder.h" />
		<Unit filename="include/FileValidator.h" />
		<Unit filename="include/HelloWorld.h" />
		<Unit filename="include/ImplementationBuilder.h" />
		<Unit filename="include/ImplementationGenerator.h">
			<Option target="default" />
			<Option target="to_codeblocks_wx28" />
			<Option target="to_codeblocks_wx30" />
		</Unit>
		<Unit filename="include/InterfaceValidator.h" />
		<Unit filename="manifest.xml" />
		<Unit filename="src/CBImplementationBuilder.cpp" />
		<Unit filename="src/FileImplementationBuilder.cpp" />
		<Unit filename="src/FileValidator.cpp" />
		<Unit filename="src/HelloWorld.cpp">
			<Option target="default" />
		</Unit>
		<Unit filename="src/ImplementationBuilder.cpp" />
		<Unit filename="src/ImplementationGenerator.cpp">
			<Option target="default" />
			<Option target="to_codeblocks_wx28" />
			<Option target="to_codeblocks_wx30" />
		</Unit>
		<Unit filename="src/InterfaceValidator.cpp" />
		<Unit filename="test/FileImplementationBuilderTest.cpp">
			<Option target="RunTest" />
		</Unit>
		<Unit filename="test/FileImplementationBuilderTest.h" />
		<Unit filename="test/MockFileValidator.h" />
		<Unit filename="test/testMain.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
